    <EmbeddedResource Remove="SummonerSpellIcons\**" />
    <None Remove="LaneIcons\**" />
    <None Remove="SummonerSpellIcons\**" />
    <Page Remove="LaneIcons\**" />
    <Page Remove="SummonerSpellIcons\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Icons\App\AppIcon.ico" />
    <None Remove="Icons\Items\IonianBootsOfLucidity.png" />
    <None Remove="Icons\Lanes\Bot.png" />
    <None Remove="Icons\Lanes\Jungle.png" />
    <None Remove="Icons\Lanes\Mid.png" />
    <None Remove="Icons\Lanes\Support.png" />
    <None Remove="Icons\Lanes\Top.png" />
    <None Remove="Icons\Runes\CosmicInsight.png" />
    <None Remove="Icons\SummonerSpells\Barrier.png" />
    <None Remove="Icons\SummonerSpells\Cleanse.png" />
