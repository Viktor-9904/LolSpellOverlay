    <None Remove="Icons\SummonerSpells\Ignite.png" />
    <None Remove="Icons\SummonerSpells\Smite.png" />
    <None Remove="Icons\SummonerSpells\Teleport.png" />
    <None Remove="Icons\SummonerSpells\TopQuestTeleport.png" />
    <None Remove="Sounds\notificationSound.wav" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Icons\App\AppIcon2.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Icons\App\AppIcon.ico">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Icons\Items\IonianBootsOfLucidity.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Icons\Lanes\Mid.png">
